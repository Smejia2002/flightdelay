        {
            "name": "1. Health Check",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "{{base_url}}/health",
                    "host": [
                        "{{base_url}}"
                    ],
                    "path": [
                        "health"
                    ]
                },
                "description": "Verifica el estado de la API y si el modelo está cargado"
            },
            "response": []
        },
        {
            "name": "2. Model Info",
            "request": {
